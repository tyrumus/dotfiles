# vim: ft=gitconfig
[user]
    email = {{ .gitemail }}
    name = {{ .gitname }}
[init]
    defaultBranch = master
[filter "lfs"]
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f
{{ if not (eq .installprofile "terminal") }}
[credential]
    helper = keepassxc --git-groups
{{- end }}
